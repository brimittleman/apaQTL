#!/bin/bash

# sbatch submission script to run main snakemake process. It then submits
# individual jobs from the compute node.


#SBATCH --job-name=snakemake
#SBATCH --output=snakelog.out
#SBATCH --time=24:00:00
#SBATCH --partition=broadwl
#SBATCH --mem=4G
#SBATCH --tasks-per-node=4


module load Anaconda3
source activate three-prime-env


bash /project2/gilad/briana/apaQTL/code/submit-snakemake.sh  "--configfile /project2/gilad/briana/apaQTL/code/config.yaml"
